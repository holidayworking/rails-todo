{
  "name": "rails-todo",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "run-p --aggregate-output --continue-on-error --print-label lint:*",
    "lint:eslint": "eslint --ext .ts,.vue app/javascript",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint 'app/javascript/**/*.vue'",
    "fix": "run-s fix:*",
    "fix:eslint": "yarn run lint:eslint --fix",
    "fix:prettier": "yarn run lint:prettier --write",
    "fix:stylelint": "yarn run lint:eslint --fix",
    "test": "jest",
    "deduplicate": "yarn-deduplicate"
  },
  "dependencies": {
    "@babel/preset-typescript": "7.13.0",
    "@rails/webpacker": "5.3.0",
    "eslint": "7.25.0",
    "html-webpack-harddisk-plugin": "2.0.0",
    "html-webpack-plugin": "4.5.2",
    "typescript": "4.2.4",
    "vue": "2.6.12",
    "vue-loader": "15.9.6",
    "vue-template-compiler": "2.6.12",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12"
  },
  "devDependencies": {
    "@prettier/plugin-ruby": "1.5.5",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "@vue/test-utils": "1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-vue": "7.9.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "prettier-plugin-sh": "0.6.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-sass-guidelines": "8.0.0",
    "ts-jest": "26.5.5",
    "vue-jest": "3.0.7",
    "webpack-dev-server": "3.11.2",
    "yarn-deduplicate": "^3.1.0"
  }
}
